<% content_for :title, 'Campus view for week ' + @panel_state.week_number.to_s + ' of ' + @panel_state.year.to_s %>
<% content_for :javascript do %>
   <%= javascript_tag do %>
	$(function() {
		$("#panel_state_date").datepicker({
          showButtonPanel: true,
          dateFormat: 'yy-mm-dd'
        });
 	});
   <% end %>
<% end %>


  <div>
    <table><tr><td>
  <a href="<%=url_for :action=>"index", :id => params[:id], :week => nil, :only_path=>true%>">Back to Effort Reports main page</a>

              </td>
    </tr></table>
  </div>

  <br/>
<div>
<%= form_for @panel_state, :as => :panel_state, :url => {:action => "campus_week", :id=> params[:id]} do |f| %>
  <table><tr>
      <td>
  Date
<%= f.text_field :date, :size => 10 %>
      </td>
       <td>
        <%= f.submit "Update" %>
      </td>
    </tr>
    <tr>
      <td>
        Year
        <%= f.text_field :year, :size => 4 %>        
        Week
        <%= f.text_field :week_number, :size => 2 %>
      </td>
       <td>
        <%= f.submit "Update" %>
      </td>


  </tr></table>
  <% end %>

