<% content_for :title, 'Show Effort Log' %>
<h1>Show Effort Log</h1>

  <p>Person: <%= @effort_log.person.human_name %> </p> 

  <p>Week <%= @effort_log.week_number %> of <%= @effort_log.year %>  </p> 

  <p>Note: The unit of measure is hours. Partial hours are ok.</p>
  <table id="effort_log_line_items">
  <%= render :partial => 'effort_log_header' %>
    <%= render :partial => 'effort_log_line_item_show', :collection => @effort_log.effort_log_line_items %>
  <tr><td colspan="3">&nbsp;</td><td colspan=7>&nbsp;</td><td class="log_sum"><%= @effort_log.sum %> hrs</td><td>&nbsp;</td></tr>
  </table>


<p><%= link_to 'Edit', edit_effort_log_path(@effort_log) %> |
<%= link_to 'Back', effort_logs_path %></p>
<br/>